{
  "name": "landing",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sir.galleto.xyz/",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "animate.css": "^3.6.1"
  },
  "scripts": {
    "start": "parcel public/index.html",
    "dev": "parcel watch public/index.html",
    "build": "parcel build public/index.html",
